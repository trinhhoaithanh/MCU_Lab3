#include "fsm_automatic.h"

void fsm_automatic_run(){
	switch(status){
	case INIT:
		//TODO
		HAL_GPIO_WritePin(LED_R, GPIO_Pin, PinState)

		status = AUTO_RED;
		setTimer1(500);
		break;
	case AUTO_RED:
		break;
	case AUTO_GREEN:
		break;
	case AUTO_YELLOW:
		break;
	default:
		break;
	}



}

